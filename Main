public class Tasksheet114 {
    public static void main(String[] args) {
       
        int check_number = 10;
            
            String result;

            for (int i = 1; i <= check_number; i++) {
                result = (i % 2 == 0) ? i + " is even number" : i + " is odd number";
                
                System.out.println(result);
            }
    }
 }
